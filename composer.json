{
    "name": "aashro/at-kider",
    "type": "typo3-cms-extension",
    "description": "Kider theme is a colorful and engaging TYPO3 template designed for kider, preschools, and early childhood education centers.",
    "authors": [
        {
            "name": "Aashro Tech",
            "email": "info@aashro.com",
            "role": "Developer",
            "homepage": "http://www.aashro.com/"
        }
    ],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^11.5",
        "mask/mask": "^8.3.11"
    },
    "autoload": {
        "psr-4": {
            "AASHRO\\Atkider\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/at-kider": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/public",
            "extension-key": "at_kider"
        }
    }
}